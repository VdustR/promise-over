{
  "name": "promise-over",
  "version": "0.1.10",
  "description": "A promise extension for over duration, timeout, and expiration.",
  "keywords": [
    "promise",
    "over",
    "timeout",
    "expire",
    "async",
    "await"
  ],
  "homepage": "https://github.com/VdustR/promise-over",
  "bugs": {
    "url": "https://github.com/VdustR/promise-over/issues"
  },
  "license": "MIT",
  "author": {
    "name": "ViPro",
    "email": "VdustR@gmail.com",
    "url": "https://VdustR.github.io"
  },
  "files": [
    "dist",
    "src"
  ],
  "main": "dist/promiseOver.umd.min.js",
  "repository": {
    "type": "git",
    "url": "git@github.com:VdustR/promise-over.git"
  },
  "scripts": {
    "commit": "git-cz",
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build --target lib --name promiseOver ./src/index.js",
    "build:docs": "vue-cli-service build --mode documentation",
    "lint": "vue-cli-service lint .",
    "deploy:docs": "cross-env BASE_URL=/promise-over/ vue-cli-service build --mode documentation ; gh-pages -d dist"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.0.1",
    "@vue/cli-plugin-eslint": "^3.0.1",
    "@vue/cli-service": "^3.0.1",
    "commitizen": "^2.10.1",
    "cross-env": "^5.2.0",
    "cz-conventional-changelog": "^2.1.0",
    "eslint-config-standard": "^12.0.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-node": "^7.0.1",
    "eslint-plugin-promise": "^4.0.0",
    "eslint-plugin-standard": "^4.0.0",
    "gh-pages": "^1.2.0",
    "node-sass": "^4.9.3",
    "normalize.css": "^8.0.0",
    "sass-loader": "^7.1.0",
    "vue": "^2.5.16",
    "vue-cli-plugin-pug": "^1.0.5",
    "vue-template-compiler": "^2.5.16"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/recommended",
      "standard"
    ],
    "rules": {},
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ],
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
